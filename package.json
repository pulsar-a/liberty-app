{
  "name": "liberty",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "license": "UNLICENSED",
  "private": true,
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d ormconfig.js",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ormconfig.js",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ormconfig.js",
    "wasm:build": "cd src/renderer/src/wasm-reader && wasm-pack build --target web --release --out-dir pkg",
    "wasm:dev": "cd src/renderer/src/wasm-reader && wasm-pack build --target web --dev --out-dir pkg",
    "wasm:watch": "cd src/renderer/src/wasm-reader && cargo watch -w src -s \"wasm-pack build --target web --dev --out-dir pkg\"",
    "wasm:clean": "cd src/renderer/src/wasm-reader && cargo clean && rm -rf pkg",
    "dev:wasm": "npm run wasm:dev && concurrently -k -n \"WASM,VITE\" -c \"yellow,cyan\" \"npm run wasm:watch\" \"npm run dev\""
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^3.0.0",
    "@headlessui/react": "^1.7.19",
    "@react-spring/web": "^9.7.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-router": "^1.15.7",
    "@tanstack/router-devtools": "^1.15.7",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@uidotdev/usehooks": "^2.4.1",
    "@xmldom/xmldom": "^0.9.5",
    "clsx": "^2.0.0",
    "date-fns": "^3.3.1",
    "electron-store": "^8.2.0",
    "electron-trpc": "^0.6.1",
    "electron-updater": "^6.3.9",
    "i18next": "^23.7.11",
    "immer": "^10.0.3",
    "node-zip": "^1.1.1",
    "radash": "^11.0.0",
    "react-i18next": "^14.0.0",
    "react-show-more-text": "^1.7.1",
    "react-spinners": "^0.13.8",
    "reflect-metadata": "^0.2.1",
    "slugify": "^1.6.6",
    "sqlite3": "^5.1.7",
    "superjson": "^1.13.3",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.3",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-show-more-text": "^1",
    "@types/validator": "^13.12.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  }
}
