{
  "name": "liberty",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "seed": "tsc ./prisma/seeds/seed.ts --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config --configName=ormconfig.js",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed --configName=ormconfig.js",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.17",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tanstack/react-query": "^4.0.0",
    "@tanstack/react-router": "^0.0.1-beta.285",
    "@tanstack/router-devtools": "^0.0.1-alpha.0",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "clsx": "^2.0.0",
    "electron-json-storage": "^4.6.0",
    "electron-settings": "^4.0.2",
    "electron-store": "^8.1.0",
    "electron-trpc": "^0.5.2",
    "electron-updater": "^6.1.1",
    "i18next": "^23.7.11",
    "immer": "^10.0.3",
    "radash": "^11.0.0",
    "react-i18next": "^14.0.0",
    "reflect-metadata": "^0.2.1",
    "sqlite3": "^5.1.7-rc.0",
    "typeorm": "^0.3.19",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@faker-js/faker": "^8.3.1",
    "@types/faker": "^6.6.9",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "faker": "^6.6.6",
    "postcss": "^8.4.32",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^5.3.3",
    "vite": "^4.4.9"
  }
}
